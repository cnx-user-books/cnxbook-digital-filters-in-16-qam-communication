                      </m:mfrac>
                      <m:mi>I</m:mi>
                      <m:mo stretchy="false">(</m:mo>
                      <m:mi>t</m:mi>
                      <m:mo stretchy="false">)</m:mo>
                      <m:mo stretchy="false">(</m:mo>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mtext>cos</m:mtext>
                      </m:mrow>
                      <m:mo stretchy="false">(</m:mo>
                      <m:mn>2ωt</m:mn>
                      <m:mo stretchy="false">)</m:mo>
                      <m:mrow>
                        <m:mo stretchy="false">)</m:mo>
                        <m:mo stretchy="false">+</m:mo>
